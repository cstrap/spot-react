{
  "name": "spot-react",
  "version": "2.0.0",
  "description": "A sample React.js application that uses Spotify API as backend",
  "main": "./bin/www",
  "scripts": {
    "build": "webpack --progress --profile --colors",
    "dev": "webpack-dev-server --hot --inline --devtool eval-source --progress --colors",
    "test": "echo \"No unit tests and e2e tests yet\" && exit 1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "jsx",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "author": "Emanuele DelBono (@emadb)",
  "license": "ISC",
  "dependencies": {
    "array.prototype.find": "^1.0.0",
    "babel-jest": "^5.3.0",
    "bootstrap": "^3.3.5",
    "chai": "^3.2.0",
    "cookie": "*",
    "errno": "^0.1.4",
    "extract-text-webpack-plugin": "^0.8.2",
    "flux": "^2.1.1",
    "grunt": "^0.4.5",
    "grunt-babel": "^5.0.1",
    "grunt-browserify": "^4.0.1",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-connect": "^0.11.2",
    "grunt-contrib-copy": "^0.8.1",
    "grunt-contrib-cssmin": "^0.13.0",
    "grunt-contrib-jshint": "^0.11.3",
    "grunt-contrib-uglify": "^0.9.2",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-mocha-test": "^0.12.7",
    "grunt-processhtml": "^0.3.8",
    "grunt-react": "^0.12.2",
    "grunt-sass": "^1.0.0",
    "jquery": "^3.0.0-alpha1",
    "lodash": "^3.10.1",
    "mocha": "^2.3.0",
    "moment": "^2.10.6",
    "numeral": "^1.5.3",
    "react": "^0.13",
    "react-router": "^0.13.3",
    "request": "^2.61.0",
    "superagent": "^1.3.0"
  },
  "devDependencies": {
    "babel-core": "^5.8.24",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.18.0",
    "file-loader": "^0.8.4",
    "node-sass": "^3.3.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.10.1"
  }
}
