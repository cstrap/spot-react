{
  "name": "spot-react",
  "version": "2.0.0",
  "description": "A sample React.js application that uses Spotify API as backend",
  "scripts": {
    "build": "webpack --progress --profile --colors",
    "dev": "webpack-dev-server --hot --inline --devtool eval-source --progress --colors",
    "test": "echo \"No unit tests and e2e tests yet\" && exit 1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "jsx",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "author": "Emanuele DelBono (@emadb)",
  "license": "ISC",
  "dependencies": {
    "node-sass": "^3.3.2",
    "array.prototype.find": "^1.0.0",
    "babel-jest": "^5.3.0",
    "bootstrap": "^3.3.5",
    "chai": "^3.2.0",
    "errno": "^0.1.4",
    "eslint": "^1.4.3",
    "eslint-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "flux": "^2.1.1",
    "jquery": "^3.0.0-alpha1",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "numeral": "^1.5.3",
    "react": "^0.13",
    "react-router": "^0.13.3",
    "superagent": "^1.3.0",
    "babel-core": "^5.8.24",
    "babel-eslint": "^4.1.2",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.18.0",
    "eslint": "^1.4.3",
    "eslint-plugin-react": "^3.3.2",
    "file-loader": "^0.8.4",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.10.1"
  }
}
